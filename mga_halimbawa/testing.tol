bagay Tao {
    edad: i32,
    tuig: u32,
}

itupad Tao {
    paraan bago(edad: i32, tuig: u32) Tao {
        ibalik Tao!(edad: edad, tuig: tuig)
    }

    paraan get_tuig(ako) u32 {
        ako.tuig
    }
}

bagay Hayop {
    edad: u8,
}

itupad i32 {
    paraan idagdag_sa(ako, iba: i32) i32 {
        ibalik ako + iba
    }

    paraan wala_lang() i32 {
        ibalik 42
    }
}

paraan generic<T, U>(a: T, b: V) T {
    ibalik a
}

paraan gen2<T>(a: T, b: Tao) {
    b.edad
}

